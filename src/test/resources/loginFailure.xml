<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- Error Handling Tests -->
<suite name="Suite">
	<groups>
		<run>
			<include name="LoginFailure"/>
		</run>
	</groups>
	
	<listeners>
        <listener class-name="resources.ExtentListeners"/>
    </listeners>
	
	<test thread-count="5" name="Login Failure Test">
    	<classes>
      		<class name="tests.LoginFailureTest"/>
    	</classes>
  	</test> 
</suite>