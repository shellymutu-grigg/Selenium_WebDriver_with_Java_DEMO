<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- E2E Tests -->
<!-- <suite parallel = "tests" name="Suite"> -->
<suite name="Suite">
	<groups>
		<run>
			<include name="Smoke"/>
		</run>
	</groups>
	
	<test thread-count="5" name="Smoke Tests">
    	<classes>
      		<class name="tests.LoginFailureTest"/>
    	</classes>
  	</test> 
	<test thread-count="5" name="Error Validations Test">
    	<classes>
      		<class name="tests.LoginSuccessTest"/>
    	</classes>
  	</test>
</suite>